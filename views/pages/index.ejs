<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../layout/head') %>
</head>
<body>
  <%- include('../layout/header') %>
  <h2> Dr.Frankenstine's, Monster Book Collection</h2>
   <div id="numOfBooks">Total Books: <%=results.length %> </div>
  <%= console.log('results.LENGTH', results.length) %>

  <section>
    <div id = "myBooks">
    <% results.forEach(book => { %>
      <img src= "<%= book.image_url %>">
      <h1><%=book.title%></h1>
      <h3><%=book.authors%></h3>
      <form>
        <button id="GhastlyViews" type="submit">VIEW DETAILS</button>
      </form>
      <% }) %>
    </div>
  </section>
</body>
</html>