<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../../layout/head') %>
</head>
<body>
  <%- include('../../layout/header') %>
 <section>
   <% searchResults.forEach(book => { %>
    <!-- <% console.log('BOOK', book); %> -->
